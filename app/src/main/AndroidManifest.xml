<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.niramoy">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Niramoy"
        tools:targetApi="31">
        <activity
            android:name="com.example.niramoy.SignUpReceiptionist"
            android:theme="@style/Theme.NoActionBar"
            android:exported="false" />
        <activity
            android:name="com.example.niramoy.SignUpNurse"
            android:theme="@style/Theme.NoActionBar"
            android:exported="false" />
        <activity
            android:name="com.example.niramoy.SignUpDoctor"
            android:theme="@style/Theme.NoActionBar"
            android:exported="false" />
        <activity
            android:name="com.example.niramoy.SignUp"
            android:theme="@style/Theme.NoActionBar"
            android:exported="false" />
        <activity
            android:name="com.example.niramoy.SignInActivity"
            android:theme="@style/Theme.NoActionBar"
            android:exported="false" />
        <activity
            android:name="com.example.niramoy.SplashScreenActivity"
            android:theme="@style/Theme.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.niramoy.SignUpDirector"
            android:theme="@style/Theme.NoActionBar"
            android:exported="false" />
        <activity
            android:name="com.example.niramoy.EnterTestReportActivity"
            android:exported="false" />
        <activity
            android:name="com.example.niramoy.MainActivity"
            android:exported="false" />
    </application>

</manifest>